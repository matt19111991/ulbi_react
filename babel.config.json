{
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-typescript",
    [
      "i18next-extract", {
        "keyAsDefaultValue": true,
        "locales": ["ru", "en"]
      }
    ]
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    [
      "@babel/preset-react", {
        "runtime": "automatic"
      }
    ]
  ]
}
