{
  "name": "ulbi_react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:client:webpack": "webpack serve --env port=3000",
    "start:client:vite": "vite",
    "start:server:dev": "NODE_ENV=development node ./json-server/index.js",
    "start:server:prod": "NODE_ENV=production node ./json-server/index.js",
    "start:vite:dev": "concurrently \"npm run start:client:vite\" \"npm run start:server:dev\"",
    "start:webpack:dev": "concurrently \"npm run start:client:webpack\" \"npm run start:server:dev\"",
    "start:vite:prod": "concurrently \"npm run start:client:vite\" \"npm run start:server:prod\"",
    "start:webpack:prod": "concurrently \"npm run start:client:webpack\" \"npm run start:server:prod\"",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "generate:template": "node ./scripts/createSlice/index.js",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "postinstall": "node ./scripts/clearCache.js",
    "prettify": "npx prettier --write **/*.{json,ts,tsx}",
    "set:feature": "ts-node scripts/removeFeature/index.ts",
    "storybook": " storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "test:e2e": "cypress open",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve --diffOnly",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "node scripts/generateVisualJsonReport.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:update": "npx loki update",
    "test:unit": "jest --config ./config/jest/jest.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
