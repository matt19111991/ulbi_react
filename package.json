    "jest-html-reporters": "^3.1.7",
    "json-server": "^0.17.4",
    "lint-staged": "^15.2.7",
    "loki": "^0.35.0",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "^3.3.2",
    "react-refresh-typescript": "^2.0.9",
    "reg-cli": "^0.18.6",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.77.5",
    "sass-loader": "^14.2.1",
    "storybook": "^8.1.9",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.1",
    "stylelint-codeguide": "^2.0.0",
    "stylelint-config-standard": "^36.0.0",
    "ts-loader": "^9.5.1",
    "ts-morph": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.3.1",
    "vite-plugin-svgr": "^4.2.0",
    "webpack": "^5.92.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "lint-staged": {
    "**/*.json": "prettier --list-different",
    "**/*.scss": "stylelint",
    "src/**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
  "loki": {
    "diffingEngine": "looks-same",
    "looks-same": {
      "ignoreCaret": true,
      "strict": false
    },
    "configurations": {
      "chrome.laptop": {
        "deviceScaleFactor": 1,
        "height": 768,
        "mobile": false,
        "target": "chrome.docker",
        "width": 1366
      },
      "chrome.iphone7": {
        "preset": "iPhone 7",
        "target": "chrome.docker"
      }
    }
  },
  "overrides": {
    "@typescript-eslint/eslint-plugin": {
      "eslint": "^9.5.0"
    },
    "eslint-plugin-import": {
      "eslint": "^9.5.0"
    },
    "eslint-plugin-jsx-a11y": {
      "eslint": "^9.5.0"
    },
    "eslint-plugin-react": {
      "eslint": "^9.5.0"
    },
    "eslint-plugin-react-hooks": {
      "eslint": "^9.5.0"
    },
    "eslint-plugin-unused-imports": {
      "eslint": "^9.5.0"
    },
    "loki": {
      "@storybook/react": "^8.1.9"
    },
    "typescript-eslint": {
      "eslint": "^9.5.0"
    }
  }
}
