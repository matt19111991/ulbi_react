{
  "name": "ulbi_react",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.tsx",
  "scripts": {
    "start:client:webpack": "cross-env NODE_OPTIONS=--no-warnings='DEP0060' webpack serve --env port=3000 --env mode=development",
    "start:client:vite": "vite",
    "start:server:dev": "cross-env NODE_ENV=development node ./json-server/index.js --watch ./json-server/db.json",
    "start:server:prod": "cross-env NODE_ENV=production node ./json-server/index.js --watch /tmp/db.json",
    "start:vite:dev": "concurrently \"npm run start:client:vite\" \"npm run start:server:dev\"",
    "start:webpack:dev": "concurrently \"npm run start:client:webpack\" \"npm run start:server:dev\"",
    "start:vite:prod": "concurrently \"npm run start:client:vite\" \"npm run start:server:prod\"",
    "start:webpack:prod": "concurrently \"npm run start:client:webpack\" \"npm run start:server:prod\"",
    "build:dev": "cross-env NODE_OPTIONS=--no-warnings='DEP0060' webpack --env mode=development",
    "build:prod": "cross-env NODE_OPTIONS=--no-warnings='DEP0060' webpack --env mode=production",
    "add:import:prefix": "ts-node scripts/add@PrefixToImports.ts",
    "create:shared:ui:public:api": "ts-node scripts/createPublicApiForSharedUI.ts",
    "generate:slice:template": "ts-node scripts/generateFsdSliceTemplate/index.ts",
    "lint:ts": "eslint \"**/*.{ts,tsx}\" && npm run types:check",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "postinstall": "ts-node scripts/clearCache.ts",
    "prettify": "npx prettier --write **/*.{json,ts,tsx}",
    "set:feature": "ts-node scripts/setFeature/index.ts",
    "storybook": "storybook dev -p 6006 -c ./config/storybook --no-open",
    "storybook:build": "storybook build -c ./config/storybook",
    "test:e2e:vite": "cypress open --config-file cypress/cypress.config.vite.ts",
    "test:e2e:webpack": "cypress open --config-file cypress/cypress.config.webpack.ts",
    "test:ui": "cross-env NODE_OPTIONS=--no-warnings npx loki test",
    "test:ui:ok": "cross-env NODE_OPTIONS=--no-warnings npx loki approve --diffOnly",
    "test:ui:ci": "cross-env NODE_OPTIONS=--no-warnings npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "ts-node scripts/generateLokiJsonReport.ts",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:update": "cross-env NODE_OPTIONS=--no-warnings npx loki update",
    "test:unit": "cross-env NODE_OPTIONS=--no-warnings='DEP0040' jest --config ./config/jest/jest.config.ts",
    "types:check": "tsc --noemit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@react-spring/rafz": "^9.7.5",
    "@react-spring/web": "^9.7.5",
    "@reduxjs/toolkit": "^2.8.1",
    "@use-gesture/react": "^10.3.1",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "i18next": "^25.1.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.1.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.0",
    "react-virtualized": "^9.22.6"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.0",
    "@storybook/addon-actions": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-viewport": "^8.6.12",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react": "^8.6.12",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7.20.5",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-virtualized": "^9.22.2",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-loader": "^10.0.0",
    "babel-plugin-i18next-extract": "^1.0.0",
    "circular-dependency-plugin": "^5.2.2",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cypress": "^14.3.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-path-checker-1911": "^0.0.25",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "globals": "^16.1.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-html-reporters": "^3.1.7",
    "json-server": "^0.17.4",
    "lint-staged": "^16.0.0",
    "loki": "^0.35.1",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "react-refresh-typescript": "^2.0.10",
    "reg-cli": "^0.18.10",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.88.0",
    "sass-loader": "^16.0.5",
    "storybook": "^8.6.12",
    "style-loader": "^4.0.0",
    "stylelint": "^16.19.1",
    "stylelint-codeguide": "^2.0.0",
    "stylelint-config-standard-scss": "^15.0.0",
    "ts-loader": "^9.5.2",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0",
    "web-push": "^3.6.7",
    "webpack": "^5.99.8",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "lint-staged": {
    "**/*.json": "prettier --list-different",
    "**/*.scss": "stylelint",
    "src/**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
  "loki": {
    "diffingEngine": "looks-same",
    "looks-same": {
      "ignoreCaret": true,
      "strict": false
    },
    "configurations": {
      "chrome.laptop": {
        "deviceScaleFactor": 1,
        "height": 768,
        "mobile": false,
        "target": "chrome.docker",
        "width": 1366
      },
      "chrome.iphone7": {
        "preset": "iPhone 7",
        "target": "chrome.docker"
      }
    }
  },
  "overrides": {
    "@react-spring/web": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "loki": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1"
    }
  }
}
